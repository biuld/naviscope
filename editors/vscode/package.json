{
  "name": "naviscope",
  "displayName": "Naviscope",
  "description": "Graph-based structured code navigation for Java",
  "version": "0.1.0",
  "publisher": "naviscope",
  "repository": {
    "type": "git",
    "url": "https://github.com/biuld/naviscope.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm run check-types && npm run lint && node esbuild.js --watch",
    "package": "vsce package",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "npm run compile && node ./dist/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/vsce": "^2.19.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.34.0",
    "glob": "^10.3.10",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
